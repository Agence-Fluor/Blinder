{
	"extends": ["./.router/tsconfig.json"],
	"compilerOptions": {
		"target": "ES2020",
		"module": "preserve",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"strict": false,
		"noEmit": true,
		"verbatimModuleSyntax": false,
		"skipLibCheck": true
	},
	"include": ["src/**/*", "src/global.d.ts"],
}
